
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // By default, deny all read/write access to all files.
    // This is the most secure posture. Your application uses Cloudinary and other
    // external services for images, so direct client access to Firebase Storage is not needed.
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
